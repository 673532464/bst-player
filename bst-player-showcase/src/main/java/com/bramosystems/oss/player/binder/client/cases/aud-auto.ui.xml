<?xml version="1.0" encoding="windows-1252"?>

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:showcase='urn:import:com.bramosystems.oss.player.binder.client'
  xmlns:player='urn:import:com.bramosystems.oss.player.uibinder.client'>

    <g:VerticalPanel spacing='10' width='80%'>
        <g:Label styleName='section-desc'>
            Using a dynamically determined media plugin
        </g:Label>
        <g:Label styleName='media-desc'>
        Use the logger to check which plugin is in use
        </g:Label>
        <showcase:CustomPlayer name='dynAuto' autoplay='false' height='50px' width='100%'
            mediaURL='GWT-HOST::media/o-na-som.mp3' />
        <showcase:Sources name='dynAuto' />
    </g:VerticalPanel>
</ui:UiBinder>
