<?xml version="1.0" encoding="windows-1252"?>

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:showcase='urn:import:com.bramosystems.oss.player.binder.client'
  xmlns:player='urn:import:com.bramosystems.oss.player.uibinder.client'>

    <g:VerticalPanel spacing='10' width='80%'>
        <g:Label styleName='section-desc'>
            Embedding video with any suitable plugin
        </g:Label>
        <g:Label styleName='media-desc'>
        Use the logger to check which plugin is in use
        </g:Label>
        <showcase:CustomPlayer name='dynvdAuto' autoplay='false' height='350px' width='100%'
            mediaURL='GWT-HOST::media/big-buck-bunny.mp4.mp3' />
        <showcase:Sources name='dynvdAuto' />
    </g:VerticalPanel>
</ui:UiBinder>
