<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:showcase='urn:import:com.bramosystems.oss.player.binder.client'
  xmlns:player='urn:import:com.bramosystems.oss.player.uibinder.client'>

    <g:VerticalPanel spacing='10' width='80%'>
        <g:Label styleName='section-desc'>
            Playlist handling using any suitable player plugin with shuffling enabled
        </g:Label>
        <player:PlaylistSupport autoplay='false' height='50px' width='100%' showLogger='true'
            mediaURL='GWT-HOST::media/o-na-som.mp3,GWT-HOST::media/thunder.mp3,GWT-HOST::media/applause.mp3' />
        <showcase:Sources name='listAuto' />
    </g:VerticalPanel>
</ui:UiBinder>
